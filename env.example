# DengueTect Environment Variables
# Copy this file to .env and fill in your values

# Flask Configuration
FLASK_DEBUG=false
SECRET_KEY=your-secret-key-here

# Database Configuration (Supabase)
# Option 1: Use full DATABASE_URL
DATABASE_URL=postgresql://postgres:your-password@your-supabase-url.supabase.co:5432/postgres

# Option 2: Use separate Supabase credentials
SUPABASE_URL=your-supabase-url.supabase.co
SUPABASE_PASSWORD=your-supabase-password

# Render Deployment
PORT=5000
